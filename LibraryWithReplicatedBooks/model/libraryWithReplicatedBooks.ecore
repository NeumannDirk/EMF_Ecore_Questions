<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="libraryWithReplicatedBooks" nsURI="http://www.kit.tva.metamodels/library"
    nsPrefix="libraryWithReplicatedBooks">
  <eClassifiers xsi:type="ecore:EClass" name="Library">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="books" upperBound="-1"
        eType="#//book/Book" containment="true"/>
  </eClassifiers>
  <eSubpackages name="book" nsURI="http://www.kit.tva.metamodels/book" nsPrefix="book">
    <eClassifiers xsi:type="ecore:EClass" name="Book">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="genre" lowerBound="1"
          upperBound="10" eType="#//book/genre/Genre"/>
    </eClassifiers>
    <eSubpackages name="genre" nsURI="http://www.kit.tva.metamodels/genre" nsPrefix="genre">
      <eClassifiers xsi:type="ecore:EEnum" name="Genre">
        <eLiterals name="FANTASY"/>
        <eLiterals name="SCIENCE_FICTION" value="1"/>
      </eClassifiers>
    </eSubpackages>
  </eSubpackages>
</ecore:EPackage>
